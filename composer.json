{
    "name": "dbmover/pgsql",
    "description": "PostgreSQL vendor-specific DbMover module",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "dbmover/core": "^0.6.4",
        "dbmover/views": "^0.6.3",
        "dbmover/pgsql-procedures": "^0.6.2",
        "dbmover/pgsql-indexes": "^0.6.3",
        "dbmover/pgsql-constraints": "^0.6.3",
        "dbmover/pgsql-triggers": "^0.6.3",
        "dbmover/pgsql-sequences": "^0.2.2",
        "dbmover/pgsql-tables": "^0.6.4"
    },
    "suggest": {
        "dbmover/pgsql-conditionals": "SQL extension to allow conditional statements in your schemas"
    },
    "require-dev": {
        "gentry/gentry": "^0.10.2"
    },
    "autoload": {
        "psr-4": {
            "Dbmover\\Pgsql\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dbmover\\Pgsql\\Test\\": "tests"
        }
    }
}
